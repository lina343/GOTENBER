{
    "name": "gotenberg-php/pdf-converter",
    "description": "A comprehensive PHP library and web application for PDF conversion using Gotenberg API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Developer",
            "email": "developer@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8",
        "symfony/console": "^6.4",
        "symfony/http-foundation": "^6.4",
        "symfony/validator": "^6.4",
        "symfony/serializer": "^6.4",
        "symfony/property-access": "^6.4",
        "monolog/monolog": "^3.5",
        "vlucas/phpdotenv": "^5.6",
        "ramsey/uuid": "^4.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.8",
        "symfony/var-dumper": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "GotenbergPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GotenbergPHP\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "phpstan": "phpstan analyse src tests --level=8",
        "cs-check": "phpcs src tests --standard=PSR12",
        "cs-fix": "phpcbf src tests --standard=PSR12",
        "quality": [
            "@cs-check",
            "@phpstan",
            "@test"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
